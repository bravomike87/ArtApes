<h1>You are about to send a request for booking <%= @artwork.title %> </h1>
<p>Please enter the dates</p>

<%= simple_form_for([@artwork, @booking]) do |f| %>
    <%= f.input :start_date, as: :date, html5: true %>
    <%= f.input :end_date, as: :date, html5: true %>
    <%= f.submit "Send Request", class: "btn btn-primary" %>
<% end %>
