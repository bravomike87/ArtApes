<h1>Landing page</h1>

<h2>login/signup on the navbar</h2>

<h2>search bar</h2>


<%= form_tag artworks_path, method: :get do %>
  <%= text_field_tag :search,
    params[:search],
    class: "form-control",
    placeholder: "Find a artwork"
  %>
  <%= submit_tag 'Search', class: "btn btn-primary"%>
<% end %>
